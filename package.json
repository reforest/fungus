    "babel-loader": "7.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-watch": "2.0.7"
  },
  "dependencies": {
